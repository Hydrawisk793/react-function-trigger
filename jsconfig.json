{
    "compilerOptions": {
        "target": "esnext",
        "lib": [
            "esnext",
            "dom",
            "dom.iterable"
        ],
        "jsx": "react",
        "baseUrl": "src",
        "moduleResolution": "node",
        "module": "commonJS",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "src"
    ]
}
